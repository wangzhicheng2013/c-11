#ifndef GETKEYOFPOA_H_
#define GETKEYOFPOA_H_

class GetKeyOfPoa
{
public:
	GetKeyOfPoa();
	virtual ~GetKeyOfPoa();
public:
	/*
	 * @purpose:get partition from poa
	 * @return true if get ok
	 * */
	virtual bool GetPartitionFromPoa(const char *poa, int partition) = 0;
};

#endif /* GETKEYOFPOA_H_ */
